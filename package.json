{
  "name": "ng-library-starter",
  "version": "0.0.1",
  "description": "Angular library starter with AOT compilation, Codelyzer, Typescript, Webpack, Karma test, vendors dependency and SCSS compilation",
  "main": "bundles/ng-library-starter.umd.js",
  "module": "index.js",
  "scripts": {
    "build": "node build.js",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist doc coverage",
    "rimraf": "rimraf",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s",
    "lint": "npm run tslint \"src/**/*.ts\" -t verbose",
    "pack-lib": "npm pack ./dist",
    "publish-lib": "npm publish ./dist",
    "test": "karma start",
    "tslint": "tslint"
  },
  "typings": "index.d.ts",
  "author": "Oleksii Trekhleb",
  "repository": {
    "type": "git",
    "url": "https://github.com/trekhleb/ng-library-starter.git"
  },
  "bugs": {
    "url": "https://github.com/trekhleb/ng-library-starter/issues"
  },
  "homepage": "https://github.com/trekhleb/ng-library-starter",
  "keywords": [
    "angular",
    "javascript",
    "typescript",
    "library"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^2.4.6",
    "@angular/core": "^2.4.6"
  },
  "devDependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/compiler-cli": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/platform-server": "2.4.6",
    "@types/jasmine": "2.5.41",
    "@types/node": "7.0.5",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "~3.0.0-beta.18",
    "chalk": "1.1.3",
    "codelyzer": "2.0.0",
    "compodoc": "0.0.41",
    "core-js": "2.4.1",
    "css-loader": "^0.26.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.3",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine-core": "2.5.2",
    "json-loader": "^0.5.4",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "2.0.2",
    "node-sass": "^4.5.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.9",
    "rimraf": "~2.6.0",
    "rollup": "0.41.4",
    "rxjs": "5.1.0",
    "sass-loader": "^6.0.0",
    "shelljs": "0.7.6",
    "source-map-loader": "^0.1.5",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.2",
    "tslint": "4.4.2",
    "typescript": "2.0.10",
    "uglify-js": "2.7.5",
    "webpack": "2.2.1",
    "zone.js": "0.7.6"
  },
  "dependencies": {
    "moment": "^2.18.0"
  }
