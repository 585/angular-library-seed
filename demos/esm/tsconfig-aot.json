{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    // "baseUrl": ".",
    "outDir": "./compiled",
    // "rootDir": "./src",
    "declaration": false,
    "sourceMap": true,
    "inlineSources": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "compileOnSave": false,
    "lib": [
      "es2015",
      "dom"
    ]
  }
}
